{
  "manifest_version": 3,
  "name": "MangaLib Extension",
  "version": "1.2.0",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mangalib.org/*",
        "https://test-front.mangalib.me/*",
        "https://v2.slashlib.me/*",
        "https://ranobelib.me/*",
        "https://hentailib.me/*",
        "https://anilib.me/*"
      ],
      "js": [],
      "world": "MAIN",
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_icon": {
      "48": "icons/icon-48.png"
    },
    "default_popup": "popup.html",
    "icon": "icons/icon-48.png"
  },
  "permissions": []
}
